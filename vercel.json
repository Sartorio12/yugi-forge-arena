{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/sync-all-cards",
      "schedule": "0 0 * * 0"
    },
    {
      "path": "/api/sync-banlist",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/sync-card-images",
      "schedule": "0 2 * * *"
    }
  ]
}