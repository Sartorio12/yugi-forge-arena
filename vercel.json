{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/daily-maintenance",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/check-live-streams",
      "schedule": "*/10 * * * *"
    }
  ]
}